 #include<stdio.h>
 #include<stdlib.h>
 void main()
 {
   int n;
   printf("Enter Size of Array");
   scanf("%d",&n);
   int *ptr;
   ptr=(int *)malloc(n*sizeof(int));
   printf(" enter the list");

   for(int i=0;i<n;i++)
   {
       scanf("%d",&ptr[i]);
   }
   for(int j=1;j<n;j++)
   {
	   int x=ptr[j];
	   for(int k=j-1;k>=0;k--)
	   {
		   if(x<ptr[k])
		   {
			   ptr[k+1]=ptr[k];
			   ptr[k]=x;
		   }
		   else
		    break;
	   }
   }
   for(int l=0;l<n;l++)
   {
	   printf("%d  ",ptr[l]);
   }
 }
